    "version": "0.2.0",
    "configurations": [
        {
            "type": "julia",
            "request": "launch",
            // You can't debug main.jl with this unfortunately as VSCode doesn't support exporting
            // environment variables in launch.json.
            "name": "run.jl",
            "program": "${workspaceFolder}/run.jl",
            "stopOnEntry": false,
            "cwd": "${workspaceFolder}",
            "juliaEnv": "${command:activeJuliaEnvironment}"
        }
    ]
}